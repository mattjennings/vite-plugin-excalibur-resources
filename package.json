{
  "name": "vite-plugin-excalibur-resources",
  "version": "0.0.0-1",
  "type": "module",
  "main": "./src/index.js",
  "exports": {
    "./loaders": {
      "import": "./src/loaders.js"
    },
    "./types": {
      "types": "./src/types.d.ts"
    },
    "./runtime": {
      "import": "./src/runtime.js"
    },
    ".": "./src/index.js"
  },
  "typesVersions": {
    "*": {
      "runtime": [
        "src/runtime.d.ts"
      ],
      "types": [
        "src/types.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --config tests/vite.config.js",
    "prepublishOnly": "test"
  },
  "peerDependencies": {
    "@excaliburjs/plugin-aseprite": "^0.27.0",
    "@excaliburjs/plugin-tiled": "^0.27.0",
    "excalibur": "^0.27.0"
  },
  "devDependencies": {
    "@babel/parser": "^7.20.7",
    "@excaliburjs/plugin-aseprite": "^0.27.0",
    "@excaliburjs/plugin-tiled": "^0.27.0",
    "excalibur": "^0.27.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.26.2"
  },
  "dependencies": {
    "dedent": "^0.7.0",
    "fast-glob": "^3.2.12",
    "query-string": "^8.1.0",
    "recast": "^0.22.0",
    "resolve.exports": "^1.1.0"
  },
  "pnpm": {
    "overrides": {
      "vite-plugin-excalibur-resources": "workspace*"
    }
  }
}
