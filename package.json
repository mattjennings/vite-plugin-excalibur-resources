{
  "name": "vite-plugin-excalibur-resources",
  "version": "0.2.0",
  "type": "module",
  "main": "./src/index.js",
  "bin": {
    "excalibur-resources": "./bin/excalibur-resources.js"
  },
  "exports": {
    "./loaders/aseprite": {
      "import": "./src/loaders/aseprite.js",
      "types": "./src/loaders/aseprite.d.ts"
    },
    "./loaders/tiled": {
      "import": "./src/loaders/tiled.js",
      "types": "./src/loaders/tiled.d.ts"
    },
    "./loaders": {
      "import": "./src/loaders.js"
    },
    "./types": {
      "types": "./src/types.d.ts"
    },
    "./runtime": {
      "import": "./src/runtime.js"
    },
    ".": "./src/index.js"
  },
  "typesVersions": {
    "*": {
      "runtime": [
        "src/runtime.d.ts"
      ],
      "types": [
        "src/types.d.ts"
      ],
      "resources": [
        "src/resources.d.ts"
      ],
      "loaders/aseprite": [
        "src/loaders/aseprite.d.ts"
      ],
      "loaders/tiled": [
        "src/loaders/tiled.d.ts"
      ]
    }
  },
  "scripts": {
    "test": "vitest --config tests/vite.config.js",
    "prepublishOnly": "npm run test -- run"
  },
  "peerDependencies": {
    "excalibur": ">=0.28"
  },
  "optionalDependencies": {
    "@excaliburjs/plugin-aseprite": ">=0.28",
    "@excaliburjs/plugin-tiled": ">=0.28"
  },
  "devDependencies": {
    "excalibur": "^0.28.6",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.26.2"
  },
  "dependencies": {
    "dedent": "^0.7.0",
    "fast-glob": "^3.2.12",
    "query-string": "^8.1.0",
    "recast": "^0.22.0",
    "@babel/parser": "^7.20.7"
  },
  "pnpm": {
    "overrides": {
      "vite-plugin-excalibur-resources": "workspace*"
    }
  }
}
